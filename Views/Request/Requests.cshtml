@model List<REQUEST>
@{
    Layout = "~/Views/Shared/_LayoutSecondary.cshtml";
}
@section Title {
    Requests
}
    @section Name1{
    tsiwtsiw
}
    @section Name2{
    niwniw
}
    <table class="table ">
        <thead>
            <tr>
                <th scope="col">To Be Paid Full</th>
                <th scope="col">To Be Paid Installment</th>
                <th scope="col">Number Of Months for installment</th>
                <th scope="col">Interest Rate</th>
                <th scope="col">Registration Date</th>
                <th scope="col">Status</th>
                <th scope="col">Debt Register</th>
                <th scope="col">Accept Request</th>
            </tr>
        </thead>
        <tbody>
        @foreach (var item in Model)
        {
            <tr class="table">
                <td>@item.ToBePaidFull</td>
                <td>@item.ToBePaidInstallment</td>
                <td>@item.NumOfMonths</td>
                <td>@item.InterestRate</td>
                <td>@item.RegDate</td>
                <td>@item.Status</td>
                <td><a asp-controller="DebtRegister" asp-action="DebtRegisterById" asp-route-id=@item.DebtRegister.Id>Debt Register</a></td>
                <td><a asp-controller="DebtRegister" asp-action="AcceptRequest"></a></td>
            </tr>
        }
    </tbody>
</table>
