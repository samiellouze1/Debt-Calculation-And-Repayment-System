@model List<REQUEST>
@{
    ViewData["Title"] = "All Requests";
}
@section Title {
    All Requests
}
    <div class="container-fluid px-0 py-5" style="max-width:1200px;margin:0 auto;">
        <div class="row mx-0 justify-content-center pt-5">
            <div class="col-lg-6">
                <div class="section-title text-center position-relative mb-4" style="margin-bottom:30px;">
                    <h6 class="d-inline-block position-relative text-secondary text-uppercase pb-2">This specific view is only viewed by admins</h6>
                    <h1 class="display-4">All Requests</h1>
                </div>
            </div>
        </div>
        <div class="row justify-content-center mx-0 mb-5">
            <div class="col-lg-12 py-6">
                <div class="table-responsive">
                    <table class="table" style="table-layout:fixed;width:100%;margin:0 auto;">
                        <thead>
                            <tr>
                                <th scope="col"></th>
                            </tr>
                        </thead>
                        <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td></td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
