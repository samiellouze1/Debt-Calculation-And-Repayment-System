@model IEnumerable<PAYMENT>
@{
    ViewData["Title"] = "Payments";
}
<table class="table " style="width:1500px;margin-left:10%;margin-right:10%;"
    <thead>
        <tr>
            <th scope="col">Amount</th>
            <th scope="col">Id</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr class="table-primary">
                <td > @item.Amount </td>
                <td>@item.Id</td>
            </tr>
        }
    </tbody>
</table>