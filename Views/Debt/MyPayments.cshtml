@model IEnumerable<PAYMENT>;
@{
    ViewData["Title"] = "My Payments";
}
<table>
    <thead>
        <tr>Id</tr>
        <tr>Amount</tr>
        <tr>ScolarshipDebtId</tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Amount</td>
                <td>@item.ScolarshipDebtId</td>
            </tr>
        }
    </tbody>
</table>