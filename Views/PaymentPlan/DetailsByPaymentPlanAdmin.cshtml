@model PAYMENTPLANFULL
@{
    ViewData["Title"] = "Payment In full Detailed";
}
<table class="table " style="width:1000px;margin-left:10%;margin-right:10%;">
    <thead>
        <tr>
            <th scope="col">Type</th>
            <th scope="col">Amount</th>
            <th scope="col">Paid</th>
            <th scope="col">Payment Date</th>
            <th scope="col">StaffMember</th>
            <th scope="col">Student</th>
        </tr>
    </thead>
    <tbody>
        <tr class="table">
            <td>@Model.Type</td>
            <td>@Model.Amount</td>
            @if (@Model.Paid == true)
            {
                <td>Yes</td>
            }
            else
            {
                <td>No</td>
            }
            <td>@Model.PaymentDate</td>
            <td>Not Implemented(StaffMember Email)</td>
            <td>Not Implemented(Student Email)</td>
        </tr>
    </tbody>
</table>